<div class="row">
    <h2>{{ adGroupTitle }}</h2>
    {%  for entity in ad  %}
        <div class="col-md-2 thumbnail">
            {% if entity.getMedia %}
                <img src="{% path entity.getMedia, 'small' %}">
            {% endif %}
            {{ entity.text|raw('html') }}
        </div>
    {% endfor %}
</div>